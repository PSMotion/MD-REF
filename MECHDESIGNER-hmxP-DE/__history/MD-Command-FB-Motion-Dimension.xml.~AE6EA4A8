            <li styleclass="HowTo Bulleted"><text styleclass="Body Text">Ein </text><text styleclass="Element">Pin-Joint</text><image src="Red-14-5.gif" scale="100.00%" styleclass="Image Caption" figurelistinclude="0"></image><text styleclass="Body Text">, der die </text><text styleclass="Interface">Startpunkte</text><text styleclass="Body Text"> (oder </text><text styleclass="Interface">Endpunkte</text><text styleclass="Body Text">) der </text><text styleclass="Element">Linien</text><image src="Red-14-3.gif" scale="100.00%" styleclass="Image Caption" figurelistinclude="0"></image><image src="Red-14-4.gif" scale="100.00%" styleclass="Image Caption" figurelistinclude="0"></image><text styleclass="Body Text"> verbindet</text></li>
          </list>
          <list id="8" type="ul" listtype="bullet" formatstring="&#183;" format-charset="SYMBOL_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="HowTo Bulleted" style="font-family:Symbol; font-size:11pt; color:#000000;">
            <li styleclass="HowTo Bulleted"><text styleclass="Body Text">Das </text><text styleclass="Element">Teil</text><image src="Red-14-1b.gif" scale="100.00%" styleclass="Image Caption" figurelistinclude="0"></image><text styleclass="Body Text"> ist </text><text styleclass="VariableName">kinematisch definiert</text><text styleclass="VariableName-IT">.</text></li>
            <li styleclass="HowTo Bulleted"><text styleclass="Body Text">Das </text><text styleclass="Element">Teil</text><image src="Red-14-2.gif" scale="100.00%" styleclass="Image Caption" figurelistinclude="0"></image><text styleclass="Body Text"> ist </text><text styleclass="VariableName" style="color:#ff0000;">nicht</text><text styleclass="Body Text">&#32;</text><text styleclass="VariableName">kinematisch definiert.</text></li>
          </list>
