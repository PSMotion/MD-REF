          <list id="8" type="ol" listtype="decimal" formatstring="&#83;&#84;&#69;&#80;&#32;&#37;&#48;&#58;&#115;&#58;" format-charset="DEFAULT_CHARSET" levelreset="true" legalstyle="false" startfrom="2" styleclass="HowTo STEP" style="font-family:&apos;Segoe UI Semibold&apos;; font-size:11pt; font-weight:bold; color:#000000;">
            <li styleclass="HowTo STEP"><text style="font-style:normal;">To </text><text styleclass="VariableName" style="color:#ff0000;">UPDATE</text><text styleclass="VariableName"> a Polyline</text></li>
          </list>
          <para styleclass="Body Text HowTo"><table styleclass="Block" rowcount="1" colcount="1">
            <tr style="vertical-align:top">
