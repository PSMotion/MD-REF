            <li styleclass="HowTo STEP"><text style="font-style:normal;">To </text><text styleclass="VariableName" style="color:#ff0000;">ADD</text><text styleclass="VariableName"> a Polyline </text></li>
          </list>
          <para styleclass="Body Text HowTo"><table styleclass="Block" rowcount="1" colcount="1">
            <tr style="vertical-align:top">
              <td>
                <list id="7" type="ol" listtype="decimal" formatstring="&#37;&#48;&#58;&#115;&#46;" format-charset="DEFAULT_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="Body Table" style="font-family:&apos;Segoe UI Semibold&apos;; font-size:11pt; font-weight:bold; color:#000000;">
                  <li styleclass="Body Table"><text style="font-style:normal;">Click</text> a <text styleclass="Element">2D-Cam</text> or <text styleclass="Element">Gear-Pair</text> in <text styleclass="Body Text">the graphic-area</text><text styleclass="VariableName">&#32;</text></li>
