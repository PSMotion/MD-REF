          <list id="2" type="ul" listtype="bullet" formatstring="·" format-charset="SYMBOL_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="Popup Box" style="font-family:Symbol; font-size:10pt; color:#000000;">
            <li styleclass="Popup Box"><text styleclass="Popup Box2" translate="true">Torque</text><text styleclass="Popup Box" translate="true"> is calculated from the </text><text styleclass="Popup Box2" translate="true">Inertia</text><text styleclass="Popup Box" translate="true"> and </text><text styleclass="Popup Box2" translate="true">Acceleration</text><text styleclass="Popup Box" translate="true"> of the </text><text styleclass="Popup Box2" translate="true">Motor</text><text styleclass="Popup Box" translate="true">, </text><text styleclass="Popup Box2" translate="true">Gearbox</text><text styleclass="Popup Box" translate="true"> and </text><text styleclass="Popup Box2" translate="true">Mechanism</text><text styleclass="Popup Box" translate="true">.</text></li>
          </list>
          <list id="3" type="ul" listtype="bullet" formatstring="·" format-charset="SYMBOL_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="Popup Box" style="font-family:Symbol; font-size:10pt; color:#000000;">
            <li styleclass="Popup Box"><text styleclass="Popup Box2" translate="true">Speed</text><text styleclass="Popup Box" translate="true"> is determined from the </text><text styleclass="Popup Box2" translate="true">mechanism</text><text styleclass="Popup Box" translate="true"> speed and the Gear-Ratio of the </text><text styleclass="Popup Box2" translate="true">gearbox</text><text styleclass="Popup Box" translate="true">.</text></li>
