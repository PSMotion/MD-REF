          <para styleclass="Body Text">Select one from:</para>
          <para styleclass="Body Text HowTo"><table styleclass="PopupTable2" rowcount="1" colcount="1" style="background-color:#ffffff;">
            <tr style="vertical-align:top">
              <td>
                <list id="3" type="ul" listtype="bullet" formatstring="&#164;" format-charset="SYMBOL_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="Bullets" style="font-family:Wingdings; font-size:11pt; color:#000000;">
                  <li styleclass="Bullets"><text styleclass="Parameter-Header">Cam-Profile</text> (default)</li>
                  <li styleclass="HowTo Bulleted" style="margin-top:0px; margin-bottom:0px; line-height:1.00;"><text styleclass="Parameter-Header">Maximum Shear-Stress</text><text styleclass="Parameter-Header" style="color:#ff0000;"> *</text><text styleclass="Parameter-Header">&#32;</text>&#32;</li>
                  <li styleclass="HowTo Bulleted" style="margin-top:0px; margin-bottom:0px; line-height:1.00;"><text styleclass="Parameter-Header">Contact Force</text><text styleclass="Parameter-Header" style="color:#ff0000;"> *</text><text styleclass="Parameter-Header">&#32;</text></li>
