          <list id="32" type="ul" listtype="bullet" formatstring="&#183;" format-charset="SYMBOL_CHARSET" levelreset="true" legalstyle="false" startfrom="1" styleclass="HowTo Bulleted" style="font-family:Symbol; font-size:11pt; color:#000000;">
            <li styleclass="HowTo Bulleted">Point and Circle: </li>
          </list>
          <para styleclass="Body Text HowTo">The Point is constrained to lie on the circumference of a Circle</para>
        </td>
        <td>
          <para styleclass="Body Text HowTo">CoincidentPtoCn</para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:21px;">
